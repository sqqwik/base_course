import matplotlib.pyplot as plt 

x = [3,8,5]
y = [7,4,9] #массивы точек
plt.plot(x, y, color="g", label="Graf 1", marker=">", ms=5) #ms маркер сайз, лейбл подпись
plt.plot(y, x, color ="r", label="Graf 2", marker="o", ms=3)


plt.xlabel("Coord: x")
plt.ylabel("Coord: y")
plt.legend() #вызов легенды #о это точка, > треугольничек
plt.title("Base") #общая подпись 
plt.grid() #подключение сетки
plt.savefig("fig_2.png")